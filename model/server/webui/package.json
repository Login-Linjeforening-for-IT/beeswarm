{
	"name": "webui",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "bash scripts/dev.sh",
		"build": "vite build && ./scripts/post-build.sh",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"reset": "rm -rf .svelte-kit node_modules",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "npm run test:ui -- --run && npm run test:client -- --run && npm run test:server -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:client": "vitest --project=client",
		"test:server": "vitest --project=server",
		"test:ui": "vitest --project=ui",
		"test:unit": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"cleanup": "rm -rf .svelte-kit build node_modules test-results"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^4.1.3",
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.562.0",
		"@playwright/test": "^1.57.0",
		"@storybook/addon-a11y": "^10.1.11",
		"@storybook/addon-docs": "^10.1.11",
		"@storybook/addon-svelte-csf": "^5.0.10",
		"@storybook/addon-vitest": "^10.1.11",
		"@storybook/sveltekit": "^10.1.11",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25",
		"@vitest/browser": "^4.0.16",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"dexie": "^4.2.1",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-storybook": "^10.1.11",
		"eslint-plugin-svelte": "^3.13.1",
		"fflate": "^0.8.2",
		"globals": "^16.5.0",
		"http-server": "^14.1.1",
		"mdast": "^3.0.0",
		"mdsvex": "^0.12.6",
		"playwright": "^1.57.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rehype-katex": "^7.0.1",
		"remark-math": "^6.0.0",
		"storybook": "^10.1.11",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"unified": "^11.0.5",
		"uuid": "^13.0.0",
		"vite": "^7.3.0",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.16",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"highlight.js": "^11.11.1",
		"mode-watcher": "^1.1.0",
		"pdfjs-dist": "^5.4.530",
		"rehype-highlight": "^7.0.2",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-breaks": "^4.0.0",
		"remark-gfm": "^4.0.1",
		"remark-html": "^16.0.1",
		"remark-rehype": "^11.1.2",
		"svelte-sonner": "^1.0.7",
		"unist-util-visit": "^5.0.0"
	}
}
